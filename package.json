{
  "name": "boilerplate-react-static",
  "version": "1.0.0",
  "description": "Boilerplate with routes, redux and styled-components for react static",
  "main": "./src/index.js",
  "scripts": {
    "start": "env-cmd ./.env npm run --silent project",
    "project": "if [[ ${NODE_ENV} == \"development\" ]]; then if [[ ${OUTPUT} == \"static\" ]]; then echo \"${RED}NODE ENVIRONMENT${WHITE}: ${LBLUE}DEVELOPMENT${NC}\n${ORANGE}PROJECT OUTPUT${WHITE}: ${LBLUE}STATIC${NC}\" && echo \"${CYAN}Remember to change .env file when building for production!${NC}\" && npm run dev:static; elif [[ ${OUTPUT} == \"standard\" ]]; then echo \"${RED}NODE ENVIRONMENT${WHITE}: ${LBLUE}DEVELOPMENT${NC}\n${ORANGE}PROJECT OUTPUT${WHITE}: ${LBLUE}STANDARD${NC}\" && echo \"${CYAN}Remember to change .env file when building for production!${NC}\" && npm run dev:standard; else echo 'Invalid OUTPUT value. Must be static or standard.'; fi; elif [[ ${NODE_ENV} == \"production\" ]]; then if [[ ${OUTPUT} == \"static\" ]]; then echo \"${RED}NODE ENVIRONMENT${WHITE}: ${LBLUE}PRODUCTION${NC}\n${ORANGE}PROJECT OUTPUT${WHITE}: ${LBLUE}STATIC${NC}\" && echo \"${CYAN}Remember to change .env file when building for production!${NC}\" && npm run build:static; elif [[ ${OUTPUT} == \"standard\" ]]; then echo \"${RED}NODE ENVIRONMENT${WHITE}: ${LBLUE}PRODUCTION${NC}\n${ORANGE}PROJECT OUTPUT${WHITE}: ${LBLUE}STANDARD${NC}\" && echo \"${CYAN}Remember to change .env file when building for production!${NC}\" && npm run build:standard; else echo 'Invalid OUTPUT value. Must be static or standard.'; fi; else echo 'Invalid NODE_ENV value. Must be development or production.'; fi",
    "dev:static": "webpack --display-error-details -w --config webpack.config.js",
    "dev:standard": "webpack --display-error-details -w --config webpack.config.js",
    "build:static": "webpack --display-error-details --config webpack.config.js -p",
    "build:standard": "webpack --display-error-details --config webpack.config.js",
    "prepublish": "pkgfiles",
    "test": "node private/jest/test.js --env=jsdom",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "storybook": "start-storybook -p 6006 -c private/storybook",
    "build-storybook": "build-storybook -c private/storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/private/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/storybook.js",
      "<rootDir>/private/jest/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules|private)/"
    ],
    "testEnvironment": "node",
    "cacheDirectory": "test/.cache",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "test/.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "globals": {},
    "notify": false,
    "resetMocks": false
  },
  "author": "TonniDev",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "dotenv": "^5.0.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash.isfunction": "^3.0.8",
    "polished": "^1.9.0",
    "react": "^16.2.0",
    "react-code-splitting": "^1.2.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.16.0",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.6.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.2.7",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "expect": "^22.1.0",
    "file-loader": "^1.1.6",
    "humps": "^2.0.1",
    "jest": "^22.4.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.4.0",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "pkgfiles": "^2.3.2",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dev-utils": "^5.0.0",
    "react-helmet": "^5.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-extension": "^2.13.2",
    "redux-devtools-log-monitor": "^1.4.0",
    "resolve-url-loader": "^2.3.0",
    "source-map-loader": "^0.2.3",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "storybook-addon-specifications": "^2.1.1",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^2.7.1"
  }
}
