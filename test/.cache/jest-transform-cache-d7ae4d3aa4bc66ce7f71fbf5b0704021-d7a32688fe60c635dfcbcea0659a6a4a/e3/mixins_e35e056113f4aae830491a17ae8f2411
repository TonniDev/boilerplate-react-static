var rem = function rem(number) {
  var tmp = typeof number === 'string' && number.indexOf('px') ? number.replace('px', '') : number;
  tmp = parseFloat(tmp);

  return tmp / 16 + 'rem';
};

var subtract1Px = function subtract1Px(number) {
  var tmp = typeof number === 'string' && number.indexOf('px') ? number.replace('px', '') : number;
  tmp = parseFloat(tmp);

  return tmp - 1 + 'px';
};

var setSizes = function setSizes(sizes) {
  var n = '';
  if (typeof sizes === 'number') {
    n += sizes + 'px';
  } else {
    sizes.forEach(function (k) {
      n += ' ' + k + 'px';
    });
  }
  return n;
};

var conflictColor = function conflictColor(bgColor) {
  var color = bgColor.charAt(0) === '#' ? bgColor.substring(1, 7) : bgColor;
  var r = parseInt(color.substring(0, 2), 16); // hexToR
  var g = parseInt(color.substring(2, 4), 16); // hexToG
  var b = parseInt(color.substring(4, 6), 16); // hexToB
  var uicolors = [r / 255, g / 255, b / 255];
  var c = uicolors.map(function (col) {
    if (col <= 0.03928) {
      return col / 12.92;
    }
    return Math.pow((col + 0.055) / 1.055, 2.4);
  });
  var L = 0.2126 * c[0] + 0.7152 * c[1] + 0.0722 * c[2];
  return L > 0.179 ? '#000' : '#FFF';
};

var calcSize = function calcSize(size) {
  switch (size) {
    case -2:
      return rem(12);
    case -1:
      return rem(14);
    case 2:
      return rem(18);
    case 3:
      return rem(24);
    case 4:
      return rem(30);
    case 5:
      return rem(40);
    case 6:
      return rem(54);
    default:
    case 1:
      return rem(16);
  }
};

var gradient = function gradient(direction, list) {
  var hex = direction + ', ';
  list.forEach(function (k, i) {
    hex += k;
    if (i !== list.length - 1) {
      hex += ', ';
    }
  });
  return hex;
};

var checkTheme = function checkTheme(props) {
  var p = props.themeColor ? 0 : props.bg;
  return p;
};

var positionArrow = function positionArrow(sTarget, sPop) {
  return sTarget < sPop;
};

var fontWeight = function fontWeight(Weight) {
  switch (Weight) {
    case 'Regular':
      return '400';
    case 'Medium':
      return '600';
    case 'Bold':
      return '700';
    default:
    case 'Light':
      return '300';
  }
};
var getResponsiveImage = function getResponsiveImage(url, size) {
  var path = url.split('.');
  return path[0] + size + '.' + path[1];
};

exports.rem = rem;
exports.subtract1Px = subtract1Px;
