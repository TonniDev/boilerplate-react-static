'use strict';

var path = require('path');

require('dotenv').config();

var rules = require('./private/webpack/rules');
var plugins = require('./private/webpack/plugins');

var outputDir = process.env.OUTPUT === 'static' ? 'static' : 'standard';
var project = process.env.OUTPUT === 'static' ? 'static' : 'standard';

module.exports = {
  context: __dirname,
  entry: './src/__' + project + '__/index.js',
  devtool: 'source-map',
  output: {
    path: path.resolve(__dirname, outputDir + '_build'),
    filename: 'bundle.js',
    libraryTarget: 'umd' // options: var, amd, umd, commonjs, commonjs2, this
  },
  module: rules,
  watch: true,
  plugins: plugins
};
